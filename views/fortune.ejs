<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Leaderboard</title>
  <style>
    body {
      font-family: Arial, sans-serif;
      background-color: #36393f;
      color: #ffffff;
    }

    .leaderboard-container {
      max-width: 800px;
      margin: 40px auto;
      padding: 20px;
      background-color: #2f3136;
      border: 1px solid #23272a;
      border-radius: 10px;
      box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
    }

    table {
      width: 100%;
      border-collapse: collapse;
    }

    th, td {
      border: 1px solid #23272a;
      padding: 10px;
      text-align: left;
    }

    th {
      background-color: #2f3136;
    }

    td img {
      width: 64px;
      height: 64px;
      border-radius: 50%;
    }

    h1 {
      color: #ffffff;
    }
  </style>
</head>
<body>
  <div class="leaderboard-container">
    <h1>Top 5 Coins Leaderboard</h1>
    <table>
      <thead>
        <tr>
          <th>Rank</th>
          <th>Username</th>
          <th>Avatar</th>
          <th>Coins</th>
        </tr>
      </thead>
      <tbody>
        <% leaderboard.forEach((user, index) => { %>
          <tr>
            <td><%= index + 1 %></td>
            <td><%= user.username %></td>
            <td><img src="<%= user.avatar %>" alt="<%= user.username %>"></td>
            <td><%= user.coins %></td>
          </tr>
        <% }); %>
      </tbody>
    </table>
  </div>
</body>
</html>